

@model Banner
@{
    ViewData["Title"] = "Create";
}
<div class="container">
    <div class="row">
        <div class="col-lg-8 mx-auto">
            <h1>Create</h1>
            <form asp-action="create" method="post" enctype="multipart/form-data">
                <div class="form-group">

                    <label asp-for="Title1">Title1</label>
                    <input class="form-control" asp-for="Title1" placeholder="Enter Title1">
                    <span class="text-danger" asp-validation-for="Title1"></span>
                </div>
                <div class="form-group">
                    <label asp-for="Title2">Title2</label>
                    <input class="form-control" asp-for="Title2" placeholder="Enter Title2">
                    <span class="text-danger" asp-validation-for="Title2"></span>
                </div>
                <div class="form-group">
                    <label asp-for="SubTitle">SubTitle</label>
                    <input class="form-control" asp-for="SubTitle" placeholder="Enter SubTitle">
                    <span class="text-danger" asp-validation-for="SubTitle"></span>
                </div>
                <div class="form-group">
                    <label asp-for="ImageFile">BackImage</label>
                    <input class="form-control" asp-for="ImageFile" placeholder="Enter BackImage">
                    <span class="text-danger" asp-validation-for="ImageFile"></span>
                </div>
                <div class="form-group">
                    <label asp-for="RedirectUrl">RedirectUrl</label>
                    <input class="form-control" asp-for="RedirectUrl" placeholder="Enter RedirectUrl">
                    <span class="text-danger" asp-validation-for="RedirectUrl"></span>
                </div>
               

                <button type="submit" class="btn btn-primary">Submit</button>
            </form>
        </div>
    </div>
</div>